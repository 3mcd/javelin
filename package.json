{
  "name": "javelin",
  "private": true,
  "author": "Eric McDaniel <eric.g.mcdaniel@gmail.com>",
  "license": "MIT",
  "version": "2.0.0-alpha.0",
  "description": "",
  "type": "module",
  "main": "index.js",
  "keywords": [],
  "scripts": {
    "test": "pnpm -r --parallel test",
    "perf": "pnpm -r --parallel perf",
    "build": "preconstruct build",
    "reset": "pnpm reset:perf & pnpm reset:dist & pnpm i",
    "reset:perf": "find ./packages -type f -name '*.perf-results' -delete",
    "reset:dist": "find ./packages -type d -name 'dist' -prune -exec rm -rf {} \\;",
    "postinstall": "preconstruct dev",
    "release": "pnpm build && pnpm publish -r"
  },
  "devDependencies": {
    "vite": "^3.2.3",
    "vitest": "^0.25.1"
  },
  "dependencies": {
    "@babel/core": "^7.20.5",
    "@babel/preset-typescript": "^7.18.6",
    "@changesets/cli": "^2.26.0",
    "@preconstruct/cli": "^2.2.2",
    "typescript": "^4.9.4"
  },
  "preconstruct": {
    "packages": ["packages/*"]
  }
}
