{
  "name": "@javelin/net",
  "author": "Eric McDaniel <eric.g.mcdaniel@gmail.com>",
  "license": "MIT",
  "version": "2.0.0-alpha.3",
  "description": "An authoritative game server and client for @javelin/ecs",
  "keywords": [
    "ecs",
    "entity-component system",
    "game",
    "gamedev",
    "multiplayer",
    "mog"
  ],
  "type": "module",
  "main": "dist/javelin-net.cjs.js",
  "module": "dist/javelin-net.esm.js",
  "exports": {
    "import": "./dist/javelin-net.esm.js",
    "require": "./dist/javelin-net.cjs.js"
  },
  "files": ["dist"],
  "scripts": {
    "test": "vitest",
    "perf": "ts-node src/index.perf.ts"
  },
  "preconstruct": {
    "entrypoints": [
      "index.ts"
    ]
  },
  "dependencies": {
    "@javelin/ecs": "workspace:*",
    "@javelin/lib": "workspace:*"
  },
  "devDependencies": {
    "@javelin/perf": "workspace:*",
    "vitest": "^0.25.1"
  }
}
