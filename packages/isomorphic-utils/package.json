  "license": "MIT",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/node/index.js",
  "browser": "dist/esm/browser/index.js",
  "types": "dist/esm/node/index.d.ts",
  "exports": {
    "require": "./dist/cjs/index.js",
    "node": "./dist/esm/node/index.js",
